{
  "manifest_version": 3,
  "name": "Course Care Notifier",
  "version": "0.0.1",
  "permissions": [
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://course.care/course/*/event/*/queue"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}